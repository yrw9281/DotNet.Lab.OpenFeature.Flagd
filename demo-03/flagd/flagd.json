{
  "$schema": "https://flagd.dev/schema/v0/flags.json",
  "flags": {
    "experiment": {
      "state": "ENABLED",
      "variants": {
        "on": true,
        "off": false
      },
      "defaultVariant": "off",
      "targeting": {
        "if": [
          {
            "ends_with": [
              {
                "var": "email"
              },
              "@test.company.com"
            ]
          },
          "on"
        ]
      }
    }
  }
}
